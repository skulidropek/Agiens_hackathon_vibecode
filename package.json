{
  "name": "ai-coding-platform-backend",
  "version": "1.0.0",
  "description": "AI Coding Platform Backend with TypeScript, Express.js, WebSocket, and Gemini CLI integration",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "npm run build && npm run lint:fix && npm run test && node dist/index.js",
    "dev": "npm run build && npm run lint:fix && npm run test && ts-node-dev --respawn --transpile-only src/index.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix"
  },
  "keywords": ["ai", "coding", "platform", "gemini", "typescript", "websocket"],
  "author": "AGI Assistant",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "ws": "^8.14.2",
    "node-pty": "^0.10.1",
    "cors": "^2.8.5",
    "uuid": "^9.0.1",
    "chokidar": "^3.5.3",
    "jsonwebtoken": "^9.0.2",
    "bcrypt": "^5.1.1",
    "helmet": "^7.1.0",
    "compression": "^1.7.4",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/ws": "^8.5.10",
    "@types/node": "^20.10.5",
    "@types/cors": "^2.8.17",
    "@types/uuid": "^9.0.7",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/bcrypt": "^5.0.2",
    "@types/compression": "^1.7.5",
    "@types/jest": "^29.5.10",
    "@types/supertest": "^2.0.16",
    "typescript": "^5.3.3",
    "ts-node-dev": "^2.0.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "supertest": "^6.3.3",
    "eslint": "^8.56.0",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
} 